<?xml version="1.0" encoding="ISO-8859-1"?>
<configuration>
  <component>
    <type>org.exoplatform.templates.groovy.GroovyTemplateService</type>
      <init-params>
        <value-param>
          <name>name</name>
          <value>false</value>
        </value-param>
      </init-params>
  </component>

  <component>
    <type>org.exoplatform.upload.UploadService</type>
    <init-params>
      <value-param>
        <name>name</name>
        <value>false</value>
      </value-param>
    </init-params>
  </component>
  
  <component>
    <type>org.exoplatform.download.DownloadService</type>
    <init-params>
      <value-param>
        <name>download.resource.cache.size</name>
        <description>Maximun number of the download can be in the cache</description>
        <value>500</value>
      </value-param>
    </init-params>
  </component>
</configuration>
