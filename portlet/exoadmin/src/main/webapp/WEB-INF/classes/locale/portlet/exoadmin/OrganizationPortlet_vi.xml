<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ##org.exoplatform.organization.webui.component.UIOrganizationPortlet
  -->
  <UIOrganizationPortlet>
    <label>
      <userManagement>Quản lí người dùng</userManagement>
      <groupManagement>Quản lí nhóm</groupManagement>
      <membershipManagement>Quản lí thành viên</membershipManagement>
    </label>
  </UIOrganizationPortlet>
  <!--
  ##org.exoplatform.organization.webui.component.UIMembershipTypeForm
  -->
  <UIMembershipTypeForm>
    <title>Thêm/Sửa Thành viên</title>
    <label>
      <name>Tên thành viên:</name>
      <description>Mô tả:</description>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.back}</Back>
      <Reset>Làm lại</Reset>
    </action>
    <msg>
      <SameName>Tên thành viên này đã tồn tại, xin vui lòng chọn một tên khác!</SameName>
    </msg>
  </UIMembershipTypeForm>
  <!--
  ##org.exoplatform.organization.webui.component.UIGroupMembershipForm
  -->
  <UIGroupEditMembershipForm>
    <label>
      <username>Tên người dùng:</username>
      <membership>Thành viên:</membership>
    </label>
    <action>
      <Save>Lưu</Save>
      <Cancel>Hủy</Cancel>
    </action>
    <msg>
      <membership-delete>Không thể lưu thành viên này, nó đã bị xóa!.</membership-delete>
      <membership-exist>Thành viên này đã tồn tại, xin vui lòng chọn thành viên khác!</membership-exist>
    </msg>
  </UIGroupEditMembershipForm>
  <EditMembership>
    <title>
      <UIGroupEditMembershipForm>Sửa thành viên</UIGroupEditMembershipForm>
      <EditMembership>Sửa Thành viên</EditMembership>
    </title>
  </EditMembership>
  <UIGroupMembershipForm>
    <label>
      <title>Thành viên nhóm</title>
      <username>Tên người dùng:</username>
      <membership>Thành viên:</membership>
      <SearchUser>Tìm kiếm người dùng</SearchUser>
      <Refresh>Refresh</Refresh>
    </label>
    <action>
      <Save>#{word.save}</Save>
    </action>
    <title>Thêm thành viên</title>
  </UIGroupMembershipForm>
  <SearchUserForm>
    <label>
      <option>
        <userName>#{word.userName}</userName>
        <firstName>#{word.firstName}</firstName>
        <lastName>#{word.lastName}</lastName>
        <email>#{word.email}</email>
      </option>
    </label>
  </SearchUserForm>
  <SearchUser>
    <title>
      <UIUserSelector>Chọn người dùng</UIUserSelector>
      <SearchUser>Chọn người dùng</SearchUser>
      <ListUserForSearch>Tìm kiếm người dùng</ListUserForSearch>
    </title>
  </SearchUser>
  <!--
  #{0} is the member that the remote memeber enter
  -->
  <UIMemberShipForm>
    <msg>
      <membershipType-exist>Loại thành viên {0} đã  tồn tại</membershipType-exist>
    </msg>
  </UIMemberShipForm>
  <!--
  ##org.exoplatform.organization.webui.component.UIMembershipForm
  -->
  <UIMembershipForm>
    <label>
      <username>Tên người dùng</username>
      <membership>Thành viên</membership>
      <name>Tên thành viên</name>
      <description>#{word.description}</description>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.back}</Back>
    </action>
  </UIMembershipForm>
  <!--
  ##org.exoplatform.organization.webui.component.UIListUser
  -->
  <UIListUsers>
    <header>
      <userName>#{word.userName}</userName>
      <lastName>#{word.lastName}</lastName>
      <firstName>#{word.firstName}</firstName>
      <email>#{word.email}</email>
      <action>#{word.action}</action>
    </header>
    <action>
      <title>
        <DeleteUser>Xóa người dùng</DeleteUser>
        <SelectUser>Chọn người dùng</SelectUser>
        <ViewUserInfo>Chỉnh sửa thông tin người dùng</ViewUserInfo>
      </title>
    </action>
    <label>
      <option>
        <userName>#{word.userName}</userName>
        <firstName>#{word.firstName}</firstName>
        <lastName>#{word.lastName}</lastName>
        <email>#{word.email}</email>
      </option>
    </label>
    <msg>
      <DeleteSuperUser>{0} là người quản trị cấp cao, nên không thể xóa được</DeleteSuperUser>
    </msg>
    <deleteUser>Bạn có chắc chắn muốn xóa người dùng {0} không?</deleteUser>
  </UIListUsers>
  <UIListMembershipType>
    <deleteMemberShip>Bạn có chắc chắn muốn xóa kiểu thành viên này không?</deleteMemberShip>
  </UIListMembershipType>
  <!--
  ##org.exoplatform.organization.webui.component.UIUserInfo
  -->
  <UIUserInfo>
    <title>Thông tin về người dùng</title>
    <tab>
      <label>
        <AccountInputSet>Thông tin tài khoản</AccountInputSet>
        <UIUserProfileInputSet>Tiểu sử người dùng</UIUserProfileInputSet>
        <UIUserMembershipSelector>Thành viên</UIUserMembershipSelector>
        <UIAccountEditInputSet>Thông tin tài khoản</UIAccountEditInputSet>
      </label>
    </tab>
    <label>
      <changePassword>Thay đổi mật khẩu:</changePassword>
      <newPassword>Mật khẩu:</newPassword>
      <confirmPassword>Xác nhận mật khẩu:</confirmPassword>
      <Profile>Tiểu sử người dùng</Profile>
      <userName>#{word.userName}:</userName>
      <password>Mật khẩu :</password>
      <Confirmpassword>Xác nhận mật khẩu:</Confirmpassword>
      <firstName>#{word.firstName}:</firstName>
      <lastName>#{word.lastName}:</lastName>
      <email>Địa chỉ hộp thư:</email>
      <HomeInfo>Thông tin về nhà ở:</HomeInfo>
      <user>
        <name>
          <given>#{word.givenName}:</given>
          <family>#{word.familyName}:</family>
          <nickName>#{word.nickName}:</nickName>
        </name>
        <bdate>#{word.birthday}:</bdate>
        <gender>#{word.gender}:</gender>
        <employer>#{word.employer}:</employer>
        <department>#{word.department}:</department>
        <jobtitle>#{word.jobTitle}:</jobtitle>
        <home-info>
          <postal>
            <name>#:</name>
            <street>#{word.street}:</street>
            <city>#{word.city}:</city>
            <stateprov>#{word.stateProv}:</stateprov>
            <postalcode>#{word.postalCode}:</postalcode>
            <country>#{word.country}:</country>
          </postal>
          <telecom>
            <mobile>
              <number>#{word.mobile}:</number>
            </mobile>
            <telephone>
              <number>#{word.tel}:</number>
            </telephone>
          </telecom>
          <online>
            <email>#{word.email}:</email>
            <uri>Đại chỉ trang web:</uri>
          </online>
        </home-info>
        <business-info>
          <postal>
            <name>#:</name>
            <city>#{word.city}:</city>
            <stateprov> Bang/hạt:</stateprov>
            <postalcode>Mã bưu điện:</postalcode>
            <country>#{word.country}:</country>
          </postal>
          <telecom>
            <mobile>
              <number>#{word.mobile}:</number>
            </mobile>
            <telephone>
              <number>#{word.tel}:</number>
            </telephone>
          </telecom>
          <online>
            <email>#{word.email}:</email>
            <uri>#{word.website}:</uri>
          </online>
        </business-info>
      </user>
      <BusinessInfo>Thông tin doanh nghiệp</BusinessInfo>
    </label>
    <action>
      <Back>#{word.cancel}</Back>
      <Save>#{word.save}</Save>
    </action>
  </UIUserInfo>
  <!--
  ##org.exoplatform.organization.webui.component.UIUserInGroup
  -->
  <UIUserInGroup>
    <confirm>
      <deleteUser>Bạn có chắc chắn muốn xóa người dùng {0} trong nhóm {1} không?</deleteUser>
    </confirm>
    <header>
      <userName>#{word.userName}</userName>
      <lastName>#{word.lastName}</lastName>
      <firstName>#{word.firstName}</firstName>
      <email>#{word.email}</email>
      <action>#{word.action}</action>
      <membershipType>Loại thành viên</membershipType>
      <lastLoginTime>Lần cuối cùng đăng nhập</lastLoginTime>
    </header>
    <label>
      <username>#{word.userName}</username>
      <membership>Tên thành viên</membership>
    </label>
    <action>
      <title>
        <DeleteUser>Xóa thành viên</DeleteUser>
        <Edit>Sửa thành viên</Edit>
      </title>
    </action>
  </UIUserInGroup>
  <!--
  ##org.exoplatform.organization.webui.component.UIGroupInfo 
  -->
  <UIGroupInfo>
    <tab>
      <label>
        <UIUserInGroup>Người dùng trong nhóm</UIUserInGroup>
      </label>
    </tab>
    <label>
      <UIUserInGroup />
    </label>
    <title>Thông tin nhóm</title>
  </UIGroupInfo>
  <!--
  ##org.exoplatform.organization.webui.component.UIMembershipList
  -->
  <UIMembershipList>
    <header>
      <name>Tên thành viên</name>
      <createdDate>Ngày tạo</createdDate>
      <modifiedDate>Ngày sửa</modifiedDate>
      <action>#{word.action}</action>
      <description>#{word.description}</description>
    </header>
    <action>
      <title>
        <EditMembership>Sửa thành viên</EditMembership>
        <DeleteMembership>Xóa thành viên</DeleteMembership>
      </title>
    </action>
    <msg>
      <InUse>Bạn không thể xóa thành viên này vì nó đang sử dụng</InUse>
    </msg>
  </UIMembershipList>
  <!--
  ##org.exoplatform.organization.webui.component.UIGroupMembershipForm
  -->
  <UIUserMembershipSelector>
    <deleteMembership>Bạn có chắc chắn muốn xóa thành viên này không?</deleteMembership>
  </UIUserMembershipSelector>
  <!--
  ##org.exoplatform.organization.webui.component.UIGroupForm
  -->
  <AddGroup>
    <title>Thêm một nhóm mới</title>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.cancel}</Back>
    </action>
  </AddGroup>
  <EditGroup>
    <title>Chỉnh sửa nhóm hiện tại</title>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.cancel}</Back>
    </action>
  </EditGroup>
  <UIGroupForm>
    <label>
      <groupName>Tên nhóm:</groupName>
      <description>Mô tả:</description>
      <label>Tên hiển thị:</label>
    </label>
    <msg>
      <group-exist>Tên nhóm này đã tồn tại, xin vui lòng chọn nhóm khác!</group-exist>
    </msg>
  </UIGroupForm>
  <!--
  ############################################################################
  #   org.exoplatform.portal.component.customization.UIShareNavigationForm   #
  ############################################################################
  -->
  <UITabPane>
    <title>
      <UISharedNavigationForm>Chia xẻ Navigation</UISharedNavigationForm>
      <UISharedPortalForm>Chia xẻ Portal</UISharedPortalForm>
      <UIUserInGroup>Thông tin nhóm</UIUserInGroup>
      <UISharedNavigation>Trang Navigation nhóm</UISharedNavigation>
    </title>
  </UITabPane>
  <UISharedNavigationForm>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.back}</Back>
      <Remove>Xóa</Remove>
    </action>
    <label>
      <navigation>Navigation</navigation>
      <description>Mô tả</description>
      <membership>Thành viên</membership>
      <priority>Độ ưu tiên</priority>
    </label>
    <tab>
      <label>
        <SharedNavigation>Thiết định chia sẻ Navigation</SharedNavigation>
        <Permission>Chọn quyền</Permission>
      </label>
    </tab>
    <msg>
      <user-nonexist>Người dùng "{0}" không tồn tại</user-nonexist>
    </msg>
  </UISharedNavigationForm>
  <UISharedNavigation>
    <msg>
      <notSelected>Bạn cần phải chọn một nhóm!</notSelected>
    </msg>
    <label>
      <userNavigation>Tên trang Navigation người dùng</userNavigation>
      <priority>Độ ưu tiên</priority>
    </label>
    <action>
      <Save>Lưu</Save>
    </action>
  </UISharedNavigation>
  <!--
  #############################################################################
  #              org.exoplatform.portal.component.customization.UISharePortalForm#
  #############################################################################
  -->
  <UISharedPortalForm>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.back}</Back>
      <Remove>Xóa</Remove>
    </action>
    <label>
      <portal>Portal</portal>
      <description>Mô tả</description>
      <membership>Thành viên</membership>
      <priority>Độ ưu tiên</priority>
    </label>
    <tab>
      <label>
        <SharedPortal>Cấu hình Portal</SharedPortal>
        <Permission>Chọn quyền</Permission>
      </label>
    </tab>
    <msg>
      <user-nonexist>Người dùng "{0}" không tồn tại</user-nonexist>
    </msg>
  </UISharedPortalForm>
  <UIGroupSharedInfo>
    <title>Chia xẻ thông tin</title>
  </UIGroupSharedInfo>
  <!--
  #############################################################################
  #           org.exoplatform.portal.organization.component.UIGroupManagement#
  #############################################################################
  -->
  <UIGroupManagement>
    <label>
      <Groups>Nhóm</Groups>
      <AddGroup>Thêm nhóm</AddGroup>
      <EditGroup>Sửa nhóm</EditGroup>
      <DeleteGroup>Xóa nhóm</DeleteGroup>
    </label>
    <deleteGroup>Bạn có chắc chắn muốn xóa nhóm này?</deleteGroup>
  </UIGroupManagement>
  <!--
  #############################################################################
  #           org.exoplatform.portal.organization.component.UISharedNavigation#
  #############################################################################
  -->
</bundle>

