<?xml version="1.0" encoding="UTF-8"?>
<bundle>
  <!--
  ##org.exoplatform.organization.webui.component.UIOrganizationPortlet
  -->
  <UIOrganizationPortlet>
    <label>
      <userManagement>ادارة المستخدمين</userManagement>
      <groupManagement>إدارة الفرق</groupManagement>
      <membershipManagement>إدارة العضوية </membershipManagement>
    </label>
  </UIOrganizationPortlet>
  <!--
  ##org.exoplatform.organization.webui.component.UIMembershipTypeForm
  -->
  <UIMembershipTypeForm>
    <title>إضافة/تحرير العضوية</title>
    <label>
      <name>اسم العضوية</name>
      <description>الوصف</description>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.back}</Back>
      <Reset>اعادة تعيين</Reset>
    </action>
    <msg>
      <SameName>هذه العضوية موجودة ، الرجاء إدخال واحدة أخرى</SameName>
    </msg>
  </UIMembershipTypeForm>
  <!--
  ##org.exoplatform.organization.webui.component.UIGroupMembershipForm
  -->
  <UIGroupEditMembershipForm>
    <label>
      <username>اسم المستخدم</username>
      <membership>العضوية</membership>
    </label>
    <action>
      <Save>احفظ</Save>
      <Cancel>الغاء</Cancel>
    </action>
    <msg>
      <membership-delete>!ا يمكن  الحفظ: إزالة عضوية</membership-delete>
      <membership-exist>!نوع العضوية موجود ، من فضلك ادخل واحد آخر</membership-exist>
    </msg>
  </UIGroupEditMembershipForm>
  <EditMembership>
    <title>
      <UIGroupEditMembershipForm>تحرير العضوية</UIGroupEditMembershipForm>
      <EditMembership>تحرير العضوية</EditMembership>
    </title>
  </EditMembership>
  <UIGroupMembershipForm>
    <label>
      <title>عضوية المجموعة</title>
      <username>اسم المستخدم</username>
      <membership>العضوية</membership>
      <SearchUser>بحث المستخدم</SearchUser>
      <Refresh>تحديث المعلومات</Refresh>
    </label>
    <action>
      <Save>#{word.save}</Save>
    </action>
    <title>أضف عضو</title>
  </UIGroupMembershipForm>
  <SearchUserForm>
    <label>
      <option>
        <userName>#{word.userName}</userName>
        <firstName>#{word.firstName}</firstName>
        <lastName>#{word.lastName}</lastName>
        <email>#{word.email}</email>
      </option>
    </label>
  </SearchUserForm>
  <SearchUser>
    <title>
      <UIUserSelector>اختر مستخدم</UIUserSelector>
      <SearchUser>اختر مستخدم</SearchUser>
      <ListUserForSearch>بحث المستخدم</ListUserForSearch>
    </title>
  </SearchUser>
  <!--
  #{0} is the member that the remote memeber enter
  -->
  <UIMemberShipForm>
    <msg>
      <membershipType-exist>نوع العضوية'(0)' مأخوذ</membershipType-exist>
    </msg>
  </UIMemberShipForm>
  <!--
  ##org.exoplatform.organization.webui.component.UIMembershipForm
  -->
  <UIMembershipForm>
    <label>
      <username>اسم المستخدم</username>
      <membership>العضوية</membership>
      <name>اسم العضوية</name>
      <description>#{word.description}</description>
    </label>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.back}</Back>
    </action>
  </UIMembershipForm>
  <!--
  ##org.exoplatform.organization.webui.component.UIListUser
  -->
  <UIListUsers>
    <header>
      <userName>#{word.userName}</userName>
      <lastName>#{word.lastName}</lastName>
      <firstName>#{word.firstName}</firstName>
      <email>#{word.email}</email>
      <action>#{word.action}</action>
    </header>
    <action>
      <title>
        <DeleteUser>حذف المستخدم</DeleteUser>
        <SelectUser>اختر المستخدم</SelectUser>
        <ViewUserInfo>تحرير معلومات المستخدم</ViewUserInfo>
      </title>
    </action>
    <label>
      <option>
        <userName>#{word.userName}</userName>
        <firstName>#{word.firstName}</firstName>
        <lastName>#{word.lastName}</lastName>
        <email>#{word.email}</email>
      </option>
    </label>
    <msg>
      <DeleteSuperUser>هو مستخدم متميز و لا يمكن حذفه</DeleteSuperUser>
    </msg>
    <deleteUser>هل أنت متأكد من حذف مستخدم {0}؟</deleteUser>
  </UIListUsers>
  <UIListMembershipType>
    <deleteMemberShip>هل أنت متأكد من حذف هذه العضوية؟</deleteMemberShip>
  </UIListMembershipType>
  <!--
  ##org.exoplatform.organization.webui.component.UIUserInfo
  -->
  <UIUserInfo>
    <title>معلومات تعريفة المستخدم</title>
    <tab>
      <label>
        <AccountInputSet>معلومات الحساب</AccountInputSet>
        <UIUserProfileInputSet>تعريفة المستخدم</UIUserProfileInputSet>
        <UIUserMembershipSelector>عضوية المستخدم</UIUserMembershipSelector>
        <UIAccountEditInputSet>معلومات الحساب</UIAccountEditInputSet>
      </label>
    </tab>
    <label>
      <changePassword>:تغيير كلمة السر</changePassword>
      <newPassword>:كلمة السر جديدة</newPassword>
      <confirmPassword>:تأكيد كلمة السر</confirmPassword>
      <Profile>تعريفة المستخدم </Profile>
      <userName>#{word.userName}:</userName>
      <password>:كلمة السر</password>
      <Confirmpassword>:تأكيد كلمة السر</Confirmpassword>
      <firstName>#{word.firstName}:</firstName>
      <lastName>#{word.lastName}:</lastName>
      <email>:البريد الالكتروني</email>
      <HomeInfo>معلومات الاقامة</HomeInfo>
      <user>
        <name>
          <given>#{word.givenName}:</given>
          <family>#{word.familyName}:</family>
          <nickName>#{word.nickName}:</nickName>
        </name>
        <bdate>#{word.birthday}:</bdate>
        <gender>#{word.gender}:</gender>
        <employer>#{word.employer}:</employer>
        <department>#{word.department}:</department>
        <jobtitle>#{word.jobTitle}:</jobtitle>
        <home-info>
          <postal>
            <name>#:</name>
            <street>#{word.street}:</street>
            <city>#{word.city}:</city>
            <stateprov>#{word.stateProv}:</stateprov>
            <postalcode>#{word.postalCode}:</postalcode>
            <country>#{word.country}:</country>
          </postal>
          <telecom>
            <mobile>
              <number>#{word.mobile}:</number>
            </mobile>
            <telephone>
              <number>#{word.tel}:</number>
            </telephone>
          </telecom>
          <online>
            <email>#{word.email}:</email>
            <uri>:الموقع</uri>
          </online>
        </home-info>
        <business-info>
          <postal>
            <name>#:</name>
            <city>#{word.city}:</city>
            <stateprov>State/Prov:</stateprov>
            <postalcode>Postal Code:</postalcode>
            <country>#{word.country}:</country>
          </postal>
          <telecom>
            <mobile>
              <number>#{word.mobile}:</number>
            </mobile>
            <telephone>
              <number>#{word.tel}:</number>
            </telephone>
          </telecom>
          <online>
            <email>#{word.email}:</email>
            <uri>#{word.website}:</uri>
          </online>
        </business-info>
      </user>
      <BusinessInfo>معلومات العمل</BusinessInfo>
    </label>
    <action>
      <Back>#{word.cancel}</Back>
      <Save>#{word.save}</Save>
    </action>
  </UIUserInfo>
  <!--
  ##org.exoplatform.organization.webui.component.UIUserInGroup
  -->
  <UIUserInGroup>
    <confirm>
      <deleteUser>هل أنت متأكد من أنك تريد حذف مستخدم (0) من المجموعة (1)؟</deleteUser>
    </confirm>
    <header>
      <userName>#{word.userName}</userName>
      <lastName>#{word.lastName}</lastName>
      <firstName>#{word.firstName}</firstName>
      <email>#{word.email}</email>
      <action>#{word.action}</action>
      <membershipType>نوع العضوية</membershipType>
      <lastLoginTime>آخر تسجيل دخول</lastLoginTime>
    </header>
    <label>
      <username>#{word.userName}</username>
      <membership>اسم العضوية</membership>
    </label>
    <action>
      <title>
        <DeleteUser>حذف عضو</DeleteUser>
        <Edit>تحرير العضو</Edit>
      </title>
    </action>
  </UIUserInGroup>
  <!--
  ##org.exoplatform.organization.webui.component.UIGroupInfo 
  -->
  <UIGroupInfo>
    <tab>
      <label>
        <UIUserInGroup>مستخدم في مجموعة</UIUserInGroup>
      </label>
    </tab>
    <label>
      <UIUserInGroup />
    </label>
    <title>معلومات المجموعة</title>
  </UIGroupInfo>
  <!--
  ##org.exoplatform.organization.webui.component.UIMembershipList
  -->
  <UIMembershipList>
    <header>
      <name>اسم العضوية</name>
      <createdDate>تاريخ الإنشاء</createdDate>
      <modifiedDate>تاريخ التعديل</modifiedDate>
      <action>#{word.action}</action>
      <description>#{word.description}</description>
    </header>
    <action>
      <title>
        <EditMembership>تحرير العضوية</EditMembership>
        <DeleteMembership>حذف العضوية</DeleteMembership>
      </title>
    </action>
    <msg>
      <InUse>لا يمكنك حذف هذه العضوية لأنها في الاستخدام</InUse>
    </msg>
  </UIMembershipList>
  <!--
  ##org.exoplatform.organization.webui.component.UIGroupMembershipForm
  -->
  <UIUserMembershipSelector>
    <deleteMembership>هل أنت متأكد من حذف هذه العضوية؟</deleteMembership>
  </UIUserMembershipSelector>
  <!--
  ##org.exoplatform.organization.webui.component.UIGroupForm
  -->
  <AddGroup>
    <title>أضف مجموعة جديدة</title>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.cancel}</Back>
    </action>
  </AddGroup>
  <EditGroup>
    <title>تحرير المجموعة الحالية</title>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.cancel}</Back>
    </action>
  </EditGroup>
  <UIGroupForm>
    <label>
      <groupName>اسم المجموعة</groupName>
      <description>الوصف</description>
      <label>العلامة</label>
    </label>
    <msg>
      <group-exist>!اسم المجموعة موجود ، من فضلك ادخل واحد آخر</group-exist>
    </msg>
  </UIGroupForm>
  <!--
  ############################################################################
  #   org.exoplatform.portal.component.customization.UIShareNavigationForm   #
  ############################################################################
  -->
  <UITabPane>
    <title>
      <UISharedNavigationForm>تصفح مشترك</UISharedNavigationForm>
      <UISharedPortalForm>بوابة مشتركة</UISharedPortalForm>
      <UIUserInGroup>معلومات المجموعة</UIUserInGroup>
      <UISharedNavigation>تصفح صفحات المجموعة</UISharedNavigation>
    </title>
  </UITabPane>
  <UISharedNavigationForm>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.back}</Back>
      <Remove>محو</Remove>
    </action>
    <label>
      <navigation>تصفح </navigation>
      <description>الوصف</description>
      <membership>العضوية</membership>
      <priority>الأولوية</priority>
    </label>
    <tab>
      <label>
        <SharedNavigation>تحديد التصفح المشترك </SharedNavigation>
        <Permission>اختيار التصريح</Permission>
      </label>
    </tab>
    <msg>
      <user-nonexist>المستخدم "(0)" لا يوجد</user-nonexist>
    </msg>
  </UISharedNavigationForm>
  <UISharedNavigation>
    <msg>
      <notSelected>!يجب ان تختار مجموعة</notSelected>
    </msg>
    <label>
      <userNavigation>اسم تصفح صفحات المستخدم</userNavigation>
      <priority>الأولوية</priority>
    </label>
    <action>
      <Save>احفظ</Save>
    </action>
  </UISharedNavigation>
  <!--
  #############################################################################
  #              org.exoplatform.portal.component.customization.UISharePortalForm#
  #############################################################################
  -->
  <UISharedPortalForm>
    <action>
      <Save>#{word.save}</Save>
      <Back>#{word.back}</Back>
      <Remove>محو</Remove>
    </action>
    <label>
      <portal>بوابة</portal>
      <description>الوصف</description>
      <membership>العضوية</membership>
      <priority>الأولوية</priority>
    </label>
    <tab>
      <label>
        <SharedPortal>تحديد البوابة</SharedPortal>
        <Permission>اختيار التصريح</Permission>
      </label>
    </tab>
    <msg>
      <user-nonexist>المستخدم "(0)" لا يوجد</user-nonexist>
    </msg>
  </UISharedPortalForm>
  <UIGroupSharedInfo>
    <title>تبادل معلومات</title>
  </UIGroupSharedInfo>
  <!--
  #############################################################################
  #           org.exoplatform.portal.organization.component.UIGroupManagement#
  #############################################################################
  -->
  <UIGroupManagement>
    <label>
      <Groups>مجموعات</Groups>
      <AddGroup>أضف مجموعة جديدة</AddGroup>
      <EditGroup>تحرير المجموعة المختارة</EditGroup>
      <DeleteGroup>حذف المجموعة المختارة</DeleteGroup>
    </label>
    <deleteGroup>هل أنت متأكد حذف هذه المجموعة؟</deleteGroup>
  </UIGroupManagement>
  <!--
  #############################################################################
  #           org.exoplatform.portal.organization.component.UISharedNavigation#
  #############################################################################
  -->
</bundle>

