<div class="UIGroupInfoContainer $uicomponent.id">	
	<div class="TitleBar"><div class="Title"><%=_ctx.appRes(uicomponent.getName() + ".title")%></div></div>
	<div class="HorizontalLayout" >
		<div class="FormContainer" >
		  <%
		      for(uiChild in uicomponent.getChildren()) { 
		        uicomponent.renderUIComponent(uiChild) ;
		      }
		  %>
		</div>
	</div>
</div>
