<div>
	<div><div style="width: 600px;" ><span style="display: none;"></span></div></div>
	<div class="UIApplicationRegistryPortlet" id="$uicomponent.id" 
		style="_width: expression(this.previousSibling.offsetWidth - 18 + 'px'); min-width: 600px;">
		<div class="UIToolbar">
	    <div class="BlueLargeToolbar">
	      <div class="ToolbarContainer">
	        <div class="LeftToolbar">
	          <div class="RightToolbar">
	            <div class="MiddleToolbar">
	             
	              <div class="ManagementIconContainer">
	               
	                <a href="<%=uicomponent.event("ViewChild", "UIApplicationOrganizer")%>" class="ItemButton">
	                	<div class="LablelIcon OrganizeIcon"><span></span></div>
	                  <div class="ButtonLabel">Organize</div>
	                </a>
	                <div class="HorizontalSeparator"><span></span></div>
	                
	                <a href="<%=uicomponent.event("ViewChild", "UIPortletManagement")%>" class="ItemButton">
