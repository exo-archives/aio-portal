<div>
	<%
		if(uicomponent.getUIComponent().getChildren().size() > 0) {
			uicomponent.renderChildren() ;
		} else {
	%>
	<div>This page content is empty. You can click on Edit Page to add content</div>
	<%}%>
</div>
<%if(uicomponent.hasBackEvent()) {%>
	<div class="HorizontalLayout">
		<div class="UIAction" style="width: 100px;">
			<div class="ActionContainer">
		    <div class="ActionButton" style="width: 90px;">
		      <div class="LightBlueStyle" >
					  <div class="ButtonLeft">
						  <div class="ButtonRight">
						    <div class="ButtonMiddle">
  						    <a href="<%=uicomponent.event("Back")%>">
  						      <%= _ctx.appRes(uicomponent.getId() + ".action.Back")%>
  						    </a>
						    </div>
					    </div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
<%}%>  