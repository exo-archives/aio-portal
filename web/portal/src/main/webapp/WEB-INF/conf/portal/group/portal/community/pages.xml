<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>
  <page>
  	<owner-type>group</owner-type>
    <owner-id>community</owner-id>
	  <name>home</name>
    <factory-id>Desktop</factory-id>
    <access-groups>/guest</access-groups>
    <show-max-window>true</show-max-window>
    
    <application>
      <instance-id>community:/site/SitePortlet/siteportlet</instance-id>
	    <application-type>portlet</application-type>
	    <title>Site portlet</title>
      <show-info-bar>false</show-info-bar>
    </application>
    
    <application>
      <instance-id>community:/content/ContentPortlet/contentportlet</instance-id>
	    <application-type>portlet</application-type>
	    <title>Content portlet</title>
      <show-info-bar>true</show-info-bar>
      <show-application-state>true</show-application-state>
      <show-application-mode>true</show-application-mode>
    </application>

    <application>
      <instance-id>exoadmin:/ecm/BrowseContentPortlet/browsecontent</instance-id>
	    <application-type>portlet</application-type>
	    <title>Browse portlet</title>
      <show-info-bar>true</show-info-bar>
    </application>

    <application>
      <instance-id>community:/exoadmin/AccountPortlet/Account</instance-id>
	    <application-type>portlet</application-type>
	    <title>Account portlet</title>
      <show-info-bar>false</show-info-bar>
    </application>  
  </page>

  <page>
  	<owner-type>group</owner-type>
    <owner-id>community</owner-id>
    <name>management</name>    
    <factory-id>Desktop</factory-id>
    <access-groups>/user</access-groups>
    <show-max-window>true</show-max-window>
    
    <application>
      <instance-id>community:/exoadmin/OrganizationPortlet/Organization</instance-id>
	    <application-type>portlet</application-type>
	    <title>Organization portlet</title>
      <show-info-bar>true</show-info-bar>
      <show-application-state>true</show-application-state>
    </application>  

    <application>
      <instance-id>community:/exoadmin/AccountPortlet/Account</instance-id>
	    <application-type>portlet</application-type>
      <title>Account portlet</title>
      <show-info-bar>false</show-info-bar>
    </application>  
  </page>
</page-set>
