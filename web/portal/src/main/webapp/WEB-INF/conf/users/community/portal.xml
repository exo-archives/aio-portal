<?xml version="1.0" encoding="ISO-8859-1"?>
<portal-config decorator="exo" width="100%" height="100%">
  <factory-id>office</factory-id>
  <owner>community</owner>
  <locale>en</locale>
  <view-permission>*:/guest</view-permission>
  
  <widget-layout>
  </widget-layout>
  
  <portal-layout>
	  <application>
	    <application-instance-id>community:/web/BannerPortlet/banner</application-instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
		
	  <container template="war:/groovy/portal/webui/component/view/UITableColumnContainer.gtmpl">
	    <application>
	      <application-instance-id>community:/web/NavigationPortlet/toolbar</application-instance-id>
		    <show-info-bar>false</show-info-bar>
		  </application>
		  <application>
	      <application-instance-id>community:/web/WelcomePortlet/welcome</application-instance-id>
	      <show-info-bar>false</show-info-bar>
		  </application>
	  </container>
		
	  <application>
	    <application-instance-id>community:/web/BreadcumbsPortlet/breacumbs</application-instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
	  
	
	  <page-body>    
	  </page-body>
	  
	  <application>
	    <application-instance-id>community:/web/FooterPortlet/footer</application-instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
  </portal-layout>
</portal-config>
