<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>
  <page>
    <owner>company</owner>
    <name>home</name>
    <title>Home</title>
    <view-permission>*:/guest</view-permission>        
    <show-max-window>true</show-max-window>
    <application>
      <title>Company site portlet</title>
	    <application-instance-id>company:/site/SitePortlet/siteportlet</application-instance-id>
	    <show-info-bar>false</show-info-bar>
	  </application>
  </page>  
  
  <page>
    <owner>company</owner>
    <name>organization</name>
    <title>Organization</title>
    <view-permission>member:/portal/company</view-permission>
    <application>
      <title>Organization portlet</title>
      <application-instance-id>company:/exoadmin/OrganizationPortlet/Organization</application-instance-id>
	  	<show-info-bar>true</show-info-bar>
	  	<show-application-state>true</show-application-state>
		</application>	
  </page>
  
  <page>
    <owner>company</owner>
    <name>myaccount</name>
    <title>My Portal</title>
    <view-permission>member:/portal/company</view-permission>
    <application>
      <title>Account portlet</title>
      <application-instance-id>company:/exoadmin/AccountPortlet/Account</application-instance-id>
      <show-info-bar>false</show-info-bar>
    </application>  
  </page> 
</page-set>
