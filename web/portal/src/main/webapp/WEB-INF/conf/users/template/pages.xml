<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>
  <page renderer="PageColumnRenderer">
    <owner>@owner@</owner>
    <name>/home</name>
    <title>Home</title>
    <viewPermission group="/guest" membership="*"/>

    <container renderer="FirstLevelTabRenderer" decorator="page" id="news">      
      <portlet decorator="community" >
        <title>News</title>
        <windowId>@owner@:/content/DisplayStaticContent/new2</windowId>
        <showInfoBar>true</showInfoBar>
        <portlet-preferences>
          <preference>
            <name>Introduction</name>
            <value>id=war:/web-content/home/news</value>
            <read-only>false</read-only>
          </preference>
          <creator>@owner@</creator>
          <modifier>@owner@</modifier>
        </portlet-preferences>
      </portlet>      
      <portlet decorator="community" >
				<title>eXo platform</title>
				<windowId>@owner@:/content/DisplayStaticContent/new1</windowId>
				<showInfoBar>true</showInfoBar>
				<portlet-preferences>
					<preference>
						<name>Introduction</name>
						<value>id=war:/web-content/home/exoplatform</value>
						<read-only>false</read-only>
					</preference>
          <creator>@owner@</creator>
          <modifier>@owner@</modifier>
				</portlet-preferences>
      </portlet>
    </container>
  </page>
</page-set>
